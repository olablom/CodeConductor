{
  "dashboard": {
    "title": "CodeConductor AI Pipeline",
    "tags": ["codeconductor", "ai", "monitoring"],
    "style": "dark",
    "timezone": "browser",
    "panels": [
      {
        "title": "Model Health Status",
        "type": "stat",
        "targets": [
          {
            "expr": "sum(codeconductor_model_healthy)",
            "legendFormat": "Healthy Models"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 3},
                {"color": "green", "value": 6}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "title": "Total Requests",
        "type": "stat",
        "targets": [
          {
            "expr": "codeconductor_total_requests",
            "legendFormat": "Total Requests"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "title": "Success Rate",
        "type": "stat",
        "targets": [
          {
            "expr": "rate(codeconductor_successful_requests[5m]) / rate(codeconductor_total_requests[5m]) * 100",
            "legendFormat": "Success Rate %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "green", "value": 90}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "title": "Uptime (Hours)",
        "type": "stat",
        "targets": [
          {
            "expr": "codeconductor_uptime_seconds / 3600",
            "legendFormat": "Uptime (hours)"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "h"
          }
        },
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "10s"
  }
} 